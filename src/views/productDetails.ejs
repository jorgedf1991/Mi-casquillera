<!DOCTYPE html>
<html lang="en">

<%- include('partials/head.ejs', {title: 'Detalles de Productos'}) %>


    <body>
        <!--Inicio cabecera-->
        <%- include('partials/header.ejs') %>
            <!--Fin cabecera-->
           
            <div id="list-user">
                <div class="panel-users">
                    <% productDetail.forEach((product) => {%>
                        <div id="box-profile-users">
                            <div class="profile-users">
                              <img src="/img/imageUsers/<%=product.image %>" alt="">  
                            </div>

                            <div class="profile-users">
                                <%= product.name %>
                            </div>

                            <div class="profile-users">
                                <%= product.price %>
                            </div>

                            <div class="profile-users">
                                <%= product.description %>
                            </div>
                        </div>
                        <% } )%>
                </div>
            </div>  
              
            <!--Inicio Footer-->
            <div class="clearfix"></div>
            <%- include('partials/footer.ejs') %>

                <!--Fin Footer-->

                <!--Script-->
                <script src="/navScript.js"></script>
    </body>

</html>